table.table
  thead
    tr
      th Number
      th Person
      th Action
      th Object
      th Edit
  tbody
    - @pao_records.each do |pao_record|
      tr
        td= link_to pao_record.pao_number, pao_record
        td= pao_record.pao_person
        td= pao_record.pao_action
        td= pao_record.pao_object
        td= link_to "edit", edit_pao_record_path(pao_record)

